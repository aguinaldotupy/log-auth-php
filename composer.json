{
    "name": "tupy/authentication-log",
    "description": "Authentication Log.",
    "license": "MIT",
    "authors": [
        {
            "name": "Aguinaldo Tupy",
            "email": "aguinaldo.tupy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/auth": "^6.0|^7.0|^8.0",
        "illuminate/bus": "^6.0|^7.0|^8.0",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/http": "^6.0|^7.0|^8.0",
        "illuminate/notifications": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5|^4.0|^5.0",
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Tupy\\AuthenticationLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tupy\\AuthenticationLog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.2-dev"
        },
        "laravel": {
            "providers": [
                "Tupy\\AuthenticationLog\\AuthenticationLogServiceProvider"
            ]
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "Required to use the Slack transport (~6.0)",
        "nexmo/client": "Required to use the Nexmo transport (~1.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
